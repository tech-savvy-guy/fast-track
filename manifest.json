{
    "manifest_version": 3,
    "name": "FastTrack",
    "version": "1.0.0",
    "description": "Finish courses you \"definitely\" started",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png",
        "default_title": "FastTrack"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.udemy.com/*",
                "https://udemy.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "activeTab"
    ],
    "host_permissions": [
        "https://www.udemy.com/*",
        "https://udemy.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}
